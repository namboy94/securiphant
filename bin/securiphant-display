import sys
from PyQt5.QtWidgets import QApplication
from securiphant.qt.MainWindow import MainWindow


def main():
    """
    This script shows a QT GUI display that visualizes the current information
    in the database

    :return: None
    """
    app = QApplication(sys.argv)
    form = MainWindow()
    form.show()
    app.exec_()


if __name__ == "__main__":
    main()
